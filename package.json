    "debug": "*",
    "depd": "*",
    "destroy": "*",
    "etag": "*",
    "express": "4.9.4",
    "finalhandler": "*",
    "http": "~0.0",
    "jade": "1.7",
    "media-typer": "*",
    "ms": "*",
    "mysql": "~2.5",
    "nconf": "*",
    "on-finished": "*",
    "proxy-addr": "*",
    "qs": "*",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "supervisor": "*",
    "uglifyify": "^3.0.1",
    "utils-merge": "*",
    "watchify": "^3.2.1",
    "when": "~3.1.0",
    "xml2js": "*",
    "xmlbuilder": "*"
  },
  "scripts": {
    "build": "browserify -t reactify -t uglifyify WRIO.jsx -o createdom.js",
