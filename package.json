{
  "name": "WRIO-InternetOS",
  "version": "1.0.10",
  "bin": "./WRIO-InternetOS/bin/WRIO-InternetOS",
  "description": "https://github.com/webRunes/WRIO-InternetOS/",
  "private": true,
  "repository": "https://github.com/webRunes/WRIO-InternetOS.git",
  "main": "WRIO-InternetOS/main.jsx",
  "engines": {
    "node": ">= v0.8.0"
  },
  "devDependencies": {
    "aws-sdk-apis": "*",
    "body-parser": "*",
    "debug": "*",
    "depd": "*",
    "destroy": "*",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^3.1.0",
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "etag": "*",
    "express": "4.9.4",
    "finalhandler": "*",
    "http": "~0.0",
    "jade": "1.7",
    "media-typer": "*",
    "ms": "*",
    "mysql": "~2.5",
    "nconf": "*",
    "on-finished": "*",
    "proxy-addr": "*",
    "qs": "*",
    "supervisor": "*",
    "utils-merge": "*",
    "when": "~3.1.0",
    "xml2js": "*",
    "xmlbuilder": "*"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "scripts": {
    "postinstall": "rm -rf node_modules/react-tools && cd node_modules && git clone https://github.com/facebook/react react-tools && cd react-tools && git reset --hard 128390a691f790b8003c7abd42560f0025e40c79 && npm i && npm run build && cd build/npm-react && npm link && cd ../../../.. && npm link react",
    "start": "npm run bundleP && gulp clear",
    "bundleP": "browserify -g [envify --NODE_ENV production] -g uglifyify WRIO-InternetOS/main.jsx -o start.js",
    "bundleD": "browserify -g [envify --NODE_ENV production] WRIO-InternetOS/main.jsx -o start.js -d",
    "bundleDOCKER": "browserify -g [envify --NODE_ENV dockerdev  --DOMAIN wrioos.local] WRIO-InternetOS/main.jsx -o start.js -d",
    "build": "npm run lint && npm run bundleD && gulp clear",
    "w": "npm run watch",
    "lint": "eslint WRIO-InternetOS",
    "watch": "watchify -p browserify-notify -g [envify --NODE_ENV development] WRIO-InternetOS/main.jsx -o start.js -d"
  },
  "dependencies": {
    "browserify": "^10.2.1",
    "browserify-notify": "^1.1.2",
    "domready": "^1.0.8",
    "envify": "^3.4.0",
    "gulp": "^3.8.11",
    "lodash.sortby": "^3.1.4",
    "npm": "^2.10.1",
    "onchange": "^1.1.0",
    "passport-signin": "git+https://git@github.com/webRunes/Login-WRIO-App.git",
    "plus": "git+https://git@github.com/webRunes/Plus-WRIO-App.git",
    "reactify": "^1.1.1",
    "reflux": "0.2.7",
    "showdown": "^1.1.0",
    "superagent": "^1.2.0",
    "titter-wrio-app": "git+https://git@github.com/webRunes/Titter-WRIO-App.git",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.1"
  }
}
