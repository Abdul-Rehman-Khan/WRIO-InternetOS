{
  "name": "WRIO-InternetOS",
  "version": "1.0.0",
  "bin": "./bin/WRIO-InternetOS",
  "description": "https://github.com/webRunes/WRIO-InternetOS/",
  "private": true,
  "repository": "https://github.com/webRunes/WRIO-InternetOS.git",
  "main": "main.jsx",
  "engines": {
    "node": ">= v0.8.0"
  },
  "devDependencies": {
    "aws-sdk-apis": "*",
    "babelify": "^6.1.2",
    "body-parser": "*",
    "debug": "*",
    "depd": "*",
    "destroy": "*",
    "etag": "*",
    "express": "4.9.4",
    "finalhandler": "*",
    "http": "~0.0",
    "jade": "1.7",
    "media-typer": "*",
    "ms": "*",
    "mysql": "~2.5",
    "nconf": "*",
    "on-finished": "*",
    "proxy-addr": "*",
    "qs": "*",
    "supervisor": "*",
    "utils-merge": "*",
    "when": "~3.1.0",
    "xml2js": "*",
    "xmlbuilder": "*"
  },
  "scripts": {
    "postinstall": "rm -rf node_modules/react-tools && cd node_modules && git clone https://github.com/facebook/react react-tools && cd react-tools && git reset --hard 128390a691f790b8003c7abd42560f0025e40c79 && npm i && npm run build && cd build/npm-react && npm link && cd ../../../.. && npm link react",
    "start": "npm run bundleP && gulp clear",
    "bundleP": "browserify -g [envify --NODE_ENV production] -t reactify -g uglifyify main.jsx -o WRIO.js",
    "bundleD": "browserify -g [envify --NODE_ENV development] -t reactify main.jsx -o WRIO.js -d",
    "build": "npm run bundleD",
    "w": "npm run watch",
    "watch": "watchify -p browserify-notify -g [envify --NODE_ENV development] -t reactify main.jsx -o WRIO.js -d"
  },
  "dependencies": {
    "bluebird": "^2.9.26",
    "browserify": "^10.2.1",
    "browserify-notify": "^1.1.2",
    "domready": "^1.0.8",
    "envify": "^3.4.0",
    "gulp": "^3.8.11",
    "node-libs-browser": "^0.5.2",
    "npm": "^2.10.1",
    "passport-signin": "git+https://git@github.com/webRunes/Login-WRIO-App.git",
    "plus": "git+https://git@github.com/webRunes/Plus-WRIO-App.git",
    "react-script-loader": "yariv/ReactScriptLoader",
    "reactify": "^1.1.1",
    "reflux": "^0.2.7",
    "superagent": "^1.2.0",
    "titter-wrio-app": "git+https://git@github.com/webRunes/Titter-WRIO-App.git",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.1"
  }
}
